{
  "multipart": [
    { "apply": { "model": "spacialblocks:block/panels/oak/fpc_vert" } },
    {
      "when": { "north": "true" },
      "apply": { "model": "spacialblocks:block/panels/oak/fpn_vert", "y": 0, "uvlock": true }
    },
    {
      "when": { "east": "true" },
      "apply": { "model": "spacialblocks:block/panels/oak/fpn_vert", "y": 90, "uvlock": true }
    },
    {
      "when": { "south": "true" },
      "apply": { "model": "spacialblocks:block/panels/oak/fpn_vert", "y": 180, "uvlock": true }
    },
    {
      "when": { "west": "true" },
      "apply": { "model": "spacialblocks:block/panels/oak/fpn_vert", "y": 270, "uvlock": true }
    },

    {
      "when": { "up": "true" },
      "apply": { "model": "spacialblocks:block/panels/oak/fpc_up_vert" }
    },
    {
      "when": { "north": "true", "up": "true" },
      "apply": { "model": "spacialblocks:block/panels/oak/fpn_up_vert", "y": 0, "uvlock": true }
    },
    {
      "when": { "east": "true", "up": "true" },
      "apply": { "model": "spacialblocks:block/panels/oak/fpn_up_vert", "y": 90, "uvlock": true }
    },
    {
      "when": { "south": "true", "up": "true" },
      "apply": { "model": "spacialblocks:block/panels/oak/fpn_up_vert", "y": 180, "uvlock": true }
    },
    {
      "when": { "west": "true", "up": "true" },
      "apply": { "model": "spacialblocks:block/panels/oak/fpn_up_vert", "y": 270, "uvlock": true }
    },

    {
      "when": { "down": "true" },
      "apply": { "model": "spacialblocks:block/panels/oak/fpc_down_vert" }
    },
    {
      "when": { "north": "true", "down": "true" },
      "apply": { "model": "spacialblocks:block/panels/oak/fpn_down_vert", "y": 0, "uvlock": true }
    },
    {
      "when": { "east": "true", "down": "true" },
      "apply": { "model": "spacialblocks:block/panels/oak/fpn_down_vert", "y": 90, "uvlock": true }
    },
    {
      "when": { "south": "true", "down": "true" },
      "apply": { "model": "spacialblocks:block/panels/oak/fpn_down_vert", "y": 180, "uvlock": true }
    },
    {
      "when": { "west": "true", "down": "true" },
      "apply": { "model": "spacialblocks:block/panels/oak/fpn_down_vert", "y": 270, "uvlock": true }
    }
  ]
}